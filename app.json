{
  "expo": {
    "name": "Seekr",
    "slug": "seekr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "supabaseUrl": "${EXPO_PUBLIC_SUPABASE_URL}",
      "supabaseAnonKey": "${EXPO_PUBLIC_SUPABASE_ANON_KEY}",
      "supabaseServiceRoleKey": "${EXPO_PUBLIC_SUPABASE_SERVICE_ROLE_KEY}",
      "pexelsApiKey": "${EXPO_PUBLIC_PEXELS_API_KEY}"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seekr.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.seekr.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "seekr",
    "config": {
      "google": {
        "scheme": "seekr",
        "redirect": {
          "ios": "seekr://auth/google/callback",
          "android": "seekr://auth/google/callback",
          "web": "seekr://auth/google/callback"
        }
      },
      "auth": {
        "redirect": {
          "ios": "seekr://auth/confirm",
          "android": "seekr://auth/confirm",
          "web": "seekr://auth/confirm"
        }
      }
    },
    "deepLinking": {
      "enabled": true,
      "schemes": ["seekr"],
      "config": {
        "screens": {
          "Auth": {
            "path": "auth/google/callback",
            "params": {
              "provider": "google"
            }
          }
        }
      }
    }
  }
} 
